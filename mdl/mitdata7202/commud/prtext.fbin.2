'<PCODE "1PRTEX">

<PACKAGE "PRTEXT"> 

<ENTRY SETUP-CAPABILITIES> 

<ENTRY PRINT-TEXT PRINT-FAX PRINT-GRAPHICS PRINT-VOICE PRINT-IMAGE 
PRINT-SEQUENTIAL PRINT-SIMULTANEOUS PRINT-INDEPENDENT PRINT-BRANCH> 

<ENTRY MEDIA-TYPES PRESENTATION-MODES> 

<ENTRY PRESENTATION-CAPABILITIES IMAGE-PRINTERS FAX-PRINTERS VOICE-PRINTERS 
GRAPHICS-PRINTERS> 

<USE "PRBG" "SCR"> 

<SETG PRESENTATION-CAPABILITIES ("RFC767")> 

<GDECL (PRESENTATION-CAPABILITIES) <LIST [REST STRING]>> 

\ 

<SETG SETUP-CAPABILITIES  %<RSUBR!- '[ %<PCODE!- "1PRTEX" 0> SETUP-CAPABILITIES 
#DECL ("VALUE" <LIST [REST STRING]> "OPTIONAL" <LIST [REST STRING]>) 
PRINT-ENCLOSURES PRINT-NOTES DO-GTTYP %<RGLOC PRESENTATION-CAPABILITIES T> %<
RGLOC STREAM T> "BITGRAPH" "BITMAP" "BRANCH" %<RGLOC PRESENTATION-MODES T> %<
RGLOC MEDIA-TYPES T> "NOTES" "ENCLOSURES" ">>Bad mode indicator: " "<<" 
">>Begin unintelligible text field<<" ">>End unintelligible text field<<" 
"[[Beginning of Sender-specified independent presentation]]" 
"[[End of Sender-specified independent presentation]]" 
"[[Beginning of Sender-specified simultaneous presentation]]" 
"[[End of Sender-specified simultaneous presentation]]" 
"[[Beginning of Sender-specified sequential presentation]]" 
"[[End of Sender-specified sequential presentation]]" "RFC767" "[Can't present "
 " in this format: " "]" "[Beginning of " " (" ") " "from file " " containing " 
" bytes" ".]" "***Can't find printer for " " in " " format?***" "[End of " 
" presentation.]" "Facsimile" %<RGLOC FAX-PRINTERS T> "Voice" %<RGLOC 
VOICE-PRINTERS T> "Image" %<RGLOC IMAGE-PRINTERS T> "Graphics" %<RGLOC 
GRAPHICS-PRINTERS T> "	**Terminal Incapable of Presenting this Object**" 
OUTCHAN "#CHANNEL [" " \"" ":<" "\""]>> 
<AND <ASSIGNED? GLUE> .GLUE <PUT ,SETUP-CAPABILITIES PGLUE ![704643072 0 0 0!]>> 


"PRINT-TEXT -- print possibly multi-media text field" 

<SETG PRINT-TEXT %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-TEXT #DECL ("VALUE" 
ANY ANY CHANNEL)] 65>> 

<SETG PRINT-INDEPENDENT %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-INDEPENDENT #
DECL ("VALUE" ATOM LIST CHANNEL)] 266>> 

<SETG PRINT-SIMULTANEOUS %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-SIMULTANEOUS #
DECL ("VALUE" ATOM LIST CHANNEL)] 322>> 

<SETG PRINT-BRANCH %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-BRANCH #DECL (
"VALUE" <OR ATOM FALSE> LIST CHANNEL)] 378>> 

<SETG PRINT-SEQUENTIAL %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-SEQUENTIAL #DECL
("VALUE" ATOM LIST CHANNEL)] 420>> 

<SETG PRESENTATION-MODES '["INDEPENDENT" PRINT-INDEPENDENT "SEQUENTIAL" 
PRINT-SEQUENTIAL "SIMULTANEOUS" PRINT-SIMULTANEOUS "BRANCH" PRINT-BRANCH]> 

<GDECL (PRESENTATION-MODES) <VECTOR [REST STRING ATOM]>> 

<SETG PRINT-MM %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-MM #DECL ("VALUE" ATOM 
ANY CHANNEL STRING VECTOR)] 476>> 

<SETG PRINT-FAX %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-FAX #DECL ("VALUE" ATOM
ANY ANY)] 719>> 

<SETG PRINT-VOICE %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-VOICE #DECL ("VALUE" 
ATOM ANY ANY)] 743>> 

<SETG PRINT-IMAGE %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-IMAGE #DECL ("VALUE" 
ATOM ANY ANY)] 767>> 

<SETG PRINT-GRAPHICS %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRINT-GRAPHICS #DECL (
"VALUE" ATOM ANY ANY)] 791>> 

<SETG MEDIA-TYPES '["FAX" PRINT-FAX "IMAGE" PRINT-IMAGE "VOICE" PRINT-VOICE 
"GRAPHICS" PRINT-GRAPHICS]> 

<GDECL (MEDIA-TYPES) <VECTOR [REST STRING ATOM]>> 

<SETG FIND-REP %<RSUBR-ENTRY '[SETUP-CAPABILITIES FIND-REP #DECL ("VALUE" <OR 
FALSE LIST> <LIST [REST STRING ANY]>)] 815>> 

<SETG IMAGE-PRINTERS ["RFC767" <> "BITGRAPH" PRESENT-BITGRAPH-IMAGE "BITMAP" 
PRESENT-BITMAP-IMAGE]> 

<SETG FAX-PRINTERS ["RFC767" <>]> 

<SETG VOICE-PRINTERS ["RFC767" <>]> 

<SETG GRAPHICS-PRINTERS ["RFC767" <>]> 

<GDECL (IMAGE-PRINTERS FAX-PRINTERS VOICE-PRINTERS GRAPHICS-PRINTERS) VECTOR> 

\ 

<SETG PRESENT-GIVE-UP %<RSUBR-ENTRY '[SETUP-CAPABILITIES PRESENT-GIVE-UP #DECL (
"VALUE" STRING ANY ANY)] 852>> 

\ 

<SETG CHANPRINT %<RSUBR-ENTRY '[SETUP-CAPABILITIES CHANPRINT #DECL ("VALUE" 
CHARACTER CHANNEL)] 869>> 

<ENDPACKAGE> 
