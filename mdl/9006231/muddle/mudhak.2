
<FLOAD "BTB;MUDREF NBIN">

<DEFINE BEGIN-MHACK ()
	<SET READ-TABLE <SETG READ-TABLE 
			  <COND (<GASSIGNED? READ-TABLE> ,READ-TABLE)
				(ELSE <IVECTOR 128 0>)>>>
	<PUT .READ-TABLE <+ <ASCII !"|> 1> ,MUDREFIN>
	<PRINTTYPE MUDREF ,OUTPUT-MUDREF>
	T>

<DEFINE END-MHACK ()
	<PUT ,READ-TABLE <+ <ASCII !"|> 1> 0>
	<PRINTTYPE MUDREF ,PRINT>
	T>

 