<SETG CNT 0>
<ON "REALT"
    <FUNCTION ()
	 <COND (<0? <MOD <SETG CNT <+ ,CNT 1>> 20>> <PRINC "(GC)"> <GC>)>
	 <PRINC !\*>>
    4>
<REALTIMER 1>

<SETG SCRATCH-SPACE <AFIND 5>>
<SETG MSG-SPACE <AFIND 5>>

<DEFINE AGCTEST ("AUX" OBJ
		(S1 ,SCRATCH-SPACE) (S2 ,MSG-SPACE) NOBJ (CNT 0))
	#DECL ((NOBJ OBJ) ANY (S1 S2) SPACE)
	<ARESET .S2 T T>
	<SET OBJ <ACOPY .S2 ,OBJECT>>
	<REPEAT ()
	<PRINT <SET CNT <+ .CNT 1>>>
	<ARESET .S1 T T>
	<COND (<SET NOBJ <AGC .S1 .OBJ>>
	       <COND (<N=? .NOBJ ,OBJECT>
		      <ERROR AGC-TO-TEMP-DECL-VIOLATION!-ERRORS .NOBJ>)>
	       <ARESET .S2 T T>
	       <COND (<SET OBJ <AGC .S2 .NOBJ>>
		      <COND (<N=? .OBJ ,OBJECT>
			     <ERROR AGC-TO-SOURCE-DECL-VIOLATION!-ERRORS .OBJ>)
			    (ELSE .OBJ)>)
		     (ELSE <ERROR AGC-TO-SOURCE-FAILED!-ERRORS .OBJ>)>)
	      (ELSE <ERROR AGC-TO-TEMP-FAILED!-ERRORS .OBJ>)>>>

<DEFINE ACOPYTEST ("AUX" (S1 ,SCRATCH-SPACE) NOBJ (CNT 0) (OBJ ,OBJECT))
	#DECL ((NOBJ OBJ) ANY (S1 S2) SPACE)
	<REPEAT ()
		<PRINT <SET CNT <+ .CNT 1>>>
		<ARESET .S1 T T>
		<COND (<SET NOBJ <ACOPY .S1 .OBJ>>
		       <COND (<N=? .NOBJ ,OBJECT>
			      <ERROR AGC-TO-TEMP-DECL-VIOLATION!-ERRORS .NOBJ>)>)
		      (ELSE <ERROR AGC-TO-TEMP-FAILED!-ERRORS .OBJ>)>>>
