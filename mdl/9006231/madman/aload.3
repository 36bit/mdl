<BLOCK (<ROOT>)>
ALOCK
<ENDBLOCK>

<DEFINE ASYLUM-LOADER ("OPTIONAL" (10X <>))
	<GROUP-LOAD "MADMAN;ASYLUM FBIN">
	<PACKAGE "ASYLUM">
	<GROUP-LOAD <COND (.10X "MADMAN;ALOCK XBIN")("MADMAN;ALOCK NBIN")>>
	<ENDPACKAGE>
	<SET AS <REST .ASYLUM <- <LENGTH .ASYLUM> 2>>>
	<SET AL <REST .ALOCK <- <LENGTH .ALOCK> 1>>>
	<PUTREST .AL <REST .AS>>
	<PUTREST .AS .ALOCK>
	<GROUP-DUMP "MADMAN;ASYLUM FBIN" ASYLUM ,PRINT>>