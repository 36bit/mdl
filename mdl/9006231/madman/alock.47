<TITLE GET-LOC>
	<DECLARE ("VALUE" UVECTOR <PRIMTYPE WORD> UVECTOR)>
	<PUSH	TP* (AB)>
	<AOBJN	AB* HERE -1>
	<PUSHJ	P* GETLOC1>
	<JRST	FINIS>

<INTERNAL-ENTRY GETLOC1 2>
	<SUBM	M* (P)>
	<MOVE	A* (TP)>
	<HLRE	B* A>
	<SUBM	A* B>
	<HRL	A* -2(TP)>
	<BLT	A*  -1(B)>
	<MOVE	A* -1(TP)>
	<MOVE	B* (TP)>
	<SUB	TP* [<4 (4)>]>
	<JRST	MPOPJ>	

<TITLE PUT-LOC>
	<DECLARE ("VALUE" <PRIMTYPE WORD> <PRIMTYPE WORD> UVECTOR)>
