<USE "MADMAN" "M-DAC">

<REALTIMER 0>
<ON "REALT" <FUNCTION () <PRINC !\*>> 2>
<REALTIMER 1>

<INIT>

<DEFINE TESTAGC ("AUX"
		(SS ,SCRATCH-SPACE) (MS ,MSG-SPACE) NOBJ MD (CNT 0))
	#DECL ((NOBJ OBJ) ANY (S1 S2) SPACE (DC) <OR FALSE FORM>)
	<PUT ,MDB 1 0>
	<MREAD <- <HIGH-MESSAGE-ID> 1>>
	<SET MD <3 ,MDB>>
	<REPEAT ()
		<PRINT <SET CNT <+ .CNT 1>>>
		<SET MD
		     <SHRINK .MD .SS .MS
			     '<VECTOR [REST STRING VECTOR]>>>
		<PUT ,MDB ,MVDATA .MD>>>
