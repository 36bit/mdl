<SET REDEFINE T>
<RSUBR-LINK <>>
<GC-MON T>
<SETG BLOAT ,TIME>

<USE "MLOAD">
<SETG LOAD-GBINS? T>

<USE "MCLEAN">

<OR <LOOKUP "GLUE" <ROOT>>
    <INSERT "GLUE" <ROOT>>>
<SET GLUE T>

<INSERT "COMPILE" <ROOT>>
<NEWTYPE IRSUBR!- LIST>
<FLOAD "MADMAN;MADASY PRELOD">

<FLOAD "MUDDLE;ELMER NBIN">
<USE "GLUE">
<SETG GLUE-MAX-SPACE T>

<SETG L-SEARCH-PATH (["READER"] ["COMMUD"] !,L-SEARCH-PATH)>
<MOBY-LOAD "READER;READER GBIN">

<SETG MACRO-SURVIVORS (GETFIX!-FIELDS!-PACKAGE)>
<SETG PACKAGE-VICTIMS ("CTABLE" "MACROS" "CHAN")>

<MOBY-CLEAN READER <>>

<USE "CLEAN" "PURITY">
<CLEANUP>
<FLUSH-CLEANUP>
<KILL:PURITY>

<PRINT <GC 0 T>>
<PRINT <GC 0 T>>

<GROUP-GLUE READER
	    <>
	    .OUTCHAN
	    ("READER" "PRTUTI")
	    (INIT-SPACES!-M-READ!-PACKAGE
	     READER-SPACES!-R-DEFS!-PACKAGE
	     MAKE-DATUM!-M-DATA!-PACKAGE
	     FGETFIX!-FIELDS!-PACKAGE
	     DATUM!-M-DATA!-PACKAGE
	     DDATUMS!-M-DATA!-PACKAGE
	     GET-DATUM!-M-DATA!-PACKAGE
	     GETFIX!-FIELDS!-PACKAGE
	     P-FORWARD!-PRTUTI!-PACKAGE
	     DO-OUTPUT!-M-DOUT!-PACKAGE
	     DO-APPLY!-M-DOUT!-PACKAGE)>

<MOBY-CLEAN READER>
<SNAME "READER">
