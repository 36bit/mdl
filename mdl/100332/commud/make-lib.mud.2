<USE "LUP">

<SET REDEFINE T>
<SETG L-SEARCH-PATH (["COMMUD"] !,L-SEARCH-PATH)>

<SETG PACKS '[
 "ACHOMP" "M-DEFS" "M-TIME" "M-PRCD" "M-DATA" "M-TEMP" "USRUTI"
 "PRTUTI" "PRTSTR" "AP"     "RQFILE" "FIELDS" "EXPUTI" "M-HNG"
 "M-TAI"  "M-READ" "M-DAC"  "M-INP"  "M-DOUT" "M-FORM" "M-MSG"
 "M-MPR"  "M-ERR"  "M-ACK"  "M-EXP"  "M-TRA"  "M-PRS"
 "M-DLV"  "M-BASE" "M-TXT"  "M-DLVO" "M-ARPANET" "M-NETS"
 "M-FIL"  "M-PER"  "M-REP"  "M-PDO"  "M-DB"   "M-DMN"  "M-COMB"
 "M-RDR"]>

<DEFINE MAKE-LIB ("AUX" R)
	<LUP-ACT "<COMMUD>LIBMUD">
	<SET R <MAPF ,LIST
	      <FUNCTION (P)
		   <PRINT .P>
		   <COND (<SET R <LUP-ADD-PACK <STRING "<COMMUD>" .P> T>>
			  <MAPRET>)
			 (T <PRIN1 .R>
			  <MAPRET .P .R>)>>
	      ,PACKS>>
	<LUP-DCT>
	.R>
 